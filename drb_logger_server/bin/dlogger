require 'pathname'
ENV['BUNDLE_GEMFILE'] ||= File.expand_path("../../Gemfile",
  Pathname.new(__FILE__).realpath)

require 'bundler/setup'
require 'drb_logger_server'
require 'dante'

Dante.run 'dlogger' do |opts|
  DrbLoggerServer.start(opts[:port], opts[:log_path])
end


